{
  "name": "gemini-node-api",
  "version": "1.0.0",
  "description": "Gemini Node.js API",
  "main": "build/index.js",
  "directories": {
    "lib": "build/src",
    "test": "test"
  },
  "type": "commonjs",
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=8.16.2"
  },
  "dependencies": {
    "@types/ws": "^6.0.4",
    "rpc-bluebird": "^1.0.4",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "typedoc": "^0.15.5",
    "typescript": "^3.7.3"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run prettier && npm run lint && npm run build && npm run prettier-write && npm run lint-fix",
    "postpublish": "rm -fr build",
    "lint": "eslint --ext .ts,.js ./",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier -c **/*.{js,md,ts,json}",
    "prettier-write": "npm run prettier -- --write",
    "generate-docs": "typedoc",
    "test": "mocha -r ts-node/register --full-trace --ui tdd --bail --extension ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vansergen/gemini-node-api.git"
  },
  "keywords": [
    "Gemini",
    "crypto",
    "exchange",
    "API"
  ],
  "author": "Sergey Bakulin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vansergen/gemini-node-api/issues"
  },
  "homepage": "https://github.com/vansergen/gemini-node-api#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "funding": {
    "type": "Coinbase Commerce",
    "url": "https://commerce.coinbase.com/checkout/04a40393-21f0-44aa-b11b-726b67f08c5a"
  }
}
